  pages = {337--348}
}

@phdthesis{lee-peer-and-self-assessment,
  comment = {
    Studied a course in which teams of 3-4 students worked together on
    projects and evaluated themselves and one another.  The evaluations were
    not on submissions or artifacts that the students submitted, but instead
    on the contributions of each team member to the overall project.  The
    thesis is full of interviews with students who express varying levels of
    comfort and utility with the reviews they received and wrote.  Emphasizes
    the value of reflection and teamwork roles.  Tons of citations on peer
    assessment and online collaborative tools.
  },
  link = "https://www.lib.utexas.edu/etd/d/2008/leeh55399/leeh55399.pdf",
  author = "Haekyung Lee",
  title = "Students’ Perceptions of Peer and Self Assessment in a Higher
  Education Online Collaborative Learning Environment",
  school = "University of Texas at Austin",
  year = 2008
}


@inproceedings{davis-shared-annotation,
  comment = {
    Studied the use of a tool called CoNote, in which students would
    collaborate/review documents by putting shared, publicly-visible
    annotations on them.  The annotations could chain like comment threads.
    Annotations were not on student work, but on the assignment descriptions,
    course notes, and problem sets.  Students could use notes to ask and
    answer questions.  A potentially interesting additional kind of rubric or
    feedback.
  },
  author = {James R. Davis and Daniel P. Huttenlocher},
  title = "Shared Annotation for Cooperative Learning",
  booktitle = "Computer Supported Collaborative Learning",
  year = 1995
}

